   "source": [
    "#Importar librerías\n",
    "import pandas as pd\n",
    "from datetime import date\n",
    "import numpy as np\n",
    "import numpy_financial as npf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "2b16eeed",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Datos para la cuenta de ahorro\n",
    "tasa_interes = 0.05  # 5% tasa de interés anual\n",
    "years = 5  # Duración del plan de ahorro en años\n",
    "Abono_year = 12  # Abonos por año (mensuales)\n",
    "inicial = 0  # Balance inicial de la cuenta\n",
    "meta = 85000  # Meta de ahorro en MXN\n",
    "fecha_inicio = date(2023, 11, 1)  # Fecha de inicio del plan de ahorro"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "02419f00",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "1604.0548597409343"
      ]
     },
     "execution_count": 17,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Cálculo de abonos mensuales iguales para llegar a la meta de ahorro en el tiempo deseado\n",
    "Abono = -npf.pmt(tasa_interes/Abono_year, years*Abono_year, (meta-inicial))\n",
    "Abono"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "8afe39d2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Rango de fechas en la tabla\n",
    "rng = pd.date_range(fecha_inicio, periods=years*Abono_year, freq='M')\n",
    "rng.name = 'Fecha del abono'\n",
    "\n",
    "# Columnas de abonos y extracto de cuenta en las fechas respectivas\n",
    "df = pd.DataFrame(index=rng, columns=['Abono', 'Balance'], dtype='float')\n",
    "\n",
    "# Para poner el índice con los periodos\n",
    "df.reset_index(inplace=True)\n",
    "df.index += 1\n",
    "df.index.name = 'Periodo'"
   ]
  },
